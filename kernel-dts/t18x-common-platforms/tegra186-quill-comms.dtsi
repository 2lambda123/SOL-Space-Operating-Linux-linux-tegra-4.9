#include <dt-bindings/gpio/tegra-gpio.h>

/ {
	/* Need to enable wifi_en pin during boot for PCIe enumeration */
	gpio: gpio@2200000 {
		gpio-init-1 = <&wlan_default>;

		wlan_default: wlan-default {
			gpio-input = <
				TEGRA_GPIO(FF, 3)
				>;
			gpio-output-high = <
				TEGRA_GPIO(N, 0)
				>;
		};
	};

	bluedroid_pm {
		compatible = "nvidia,tegra-bluedroid_pm";
		id = <0>;
		bluedroid_pm,reset-gpio = <&gpio TEGRA_GPIO(L, 7) 0>;
		bluedroid_pm,host-wake-gpio = <&gpio TEGRA_GPIO(FF, 4) 0>;
		bluedroid_pm,ext-wake-gpio = <&gpio TEGRA_GPIO(Y, 4) 0>;
		interrupt-parent = <&gpio>;
		interrupts = <TEGRA_GPIO(FF, 4) 0x01>;
	};

	bcmdhd_wlan {
		compatible = "android,bcmdhd_wlan";
		interrupt-parent = <&gpio>;
		interrupts = <TEGRA_GPIO(FF, 3) 0x14>;
		wlan-pwr-gpio = <&gpio TEGRA_GPIO(N, 0) 0>;
		status = "okay";
	};
};
