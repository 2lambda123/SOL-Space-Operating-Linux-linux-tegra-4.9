#include "tegra194-cpuidle.dtsi"

/ {
	cpus {
		#address-cells = <2>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a57-64bit", "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x10000>;
			enable-method = "psci";
			cpu-idle-states = <&C6 &C7>;
		};

		cpu@1 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x10001>;
			enable-method = "psci";
			status = "disabled";
		};

		cpu@2 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x100>;
			enable-method = "psci";
			status = "disabled";
		};

		cpu@3 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x101>;
			enable-method = "psci";
			status = "disabled";
		};

		cpu@4 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x200>;
			enable-method = "psci";
			status = "disabled";
		};

		cpu@5 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x201>;
			enable-method = "psci";
			status = "disabled";
		};

		cpu@6 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x10300>;
			enable-method = "psci";
			status = "disabled";
		};

		cpu@7 {
			device_type = "cpu";
			compatible = "nvidia,carmel", "arm,armv8";
			reg = <0x0 0x10301>;
			enable-method = "psci";
			status = "disabled";
		};

	};

       	arm-pmu {
		compatible = "arm,armv8-pmuv3";
		interrupts = <0 384 0x4>,
			     <0 385 0x4>,
			     <0 386 0x4>,
			     <0 387 0x4>,
			     <0 388 0x4>,
			     <0 389 0x4>,
			     <0 390 0x4>,
			     <0 391 0x4>;
		status = "disabled";
	};
};
