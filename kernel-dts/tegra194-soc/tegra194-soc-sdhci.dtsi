/ {

	aliases {
		sdhci3 = &sdmmc4;
	};

	sdmmc4: sdhci@3460000 { /* Used for eMMC */
		compatible = "nvidia,tegra194-sdhci";
		reg = <0x0 0x3460000 0x0 0x210>;
		interrupts = < 0 65 0x04>;
		max-clk-limit = <196249804>;
		ddr-clk-limit = <48000000>;
		tap-delay = <9>;
		trim-delay = <5>;
		nvidia,ddr-tap-delay = <9>;
		ddr-trim-delay = <5>;
		dqs-trim-delay = <63>;
		dqs-trim-delay-hs533 = <40>;
		bus-width = <8>;
		ignore-pm-notify;
		keep-power-in-suspend;
		non-removable;
		cap-mmc-highspeed;
		cap-sd-highspeed;
		mmc-ddr-1_8v;
		mmc-hs200-1_8v;
		mmc-hs400-1_8v;
		compad-vref-3v3 = <0x7>;
		compad-vref-1v8 = <0x7>;
		uhs-mask = <0x60>;
		nvidia,is-emmc;
		calib-3v3-offsets = <0x0505>;
		calib-1v8-offsets = <0x0505>;
		reset-names = "sdmmc";
		clock-names = "sdmmc", "pll_p", "pll_c4_out0";
		#stream-id-cells = <1>;
		status = "disabled";
	};
};
