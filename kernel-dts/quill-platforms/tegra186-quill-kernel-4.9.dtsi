/ {
	reserved-memory {
		generic_carveout {
			size = <0x0 0x40000000>; /* 1GB generic carveout */
			status = "okay";
		};

		vpr-carveout {
			alloc-ranges = <0x0 0x0 0x2 0x00000000>;
			status = "okay";
		};
	};

	tegra-carveouts {
		status = "okay";
	};

	miscreg@00100000 {
		compatible = "nvidia,tegra186-miscreg";
		reg = <0x0 0x00100000 0x0 0xf000>, /* Chipid */
			<0x0 0x0010f000 0x0 0x1000>; /* Straps */
	};
};
